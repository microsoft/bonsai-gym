{
	"name": "Existing Dockerfile",
	"context": "..",
	"dockerFile": "../Dockerfile",

	// Uncomment this line if you want to pass in environment variables through a file
	//"runArgs": [ "--env-file", "path\to\you\.env\file" ],

	// Environment variables
	"containerEnv": {
		"SIM_ACCESS_KEY": "${localEnv:SIM_ACCESS_KEY}",
		"SIM_WORKSPACE": "${localEnv:SIM_WORKSPACE}",
		"SIM_ACR_PATH": "${localEnv:SIM_ACR_PATH}"
	},

	// Extensions to add on startup
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python"
			]
		}
	}
}
